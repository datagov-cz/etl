            <artifactId>rdf4j-repository-sparql</artifactId>
        </dependency>
        <!-- org.wikidata -->
        <dependency>
            <groupId>org.wikidata.wdtk</groupId>
            <artifactId>wdtk-datamodel</artifactId>
